{
    "name": "baka/phalcon-sqlsrv",
    "description": "Baka Component for Phalcon - MS SQL Server (PDO) Adapter",
    "keywords": ["framework", "phalcon", "sqlsrv"],
    "authors": [{
        "name": "ToNict",
        "email": "shinobi.wk@hotmail.com"
    }],
    "support": {
        "issues": "https://github.com/phalcon/incubator/issues",
        "source": "https://github.com/phalcon/incubator",
        "forum": "https://forum.phalconphp.com/",
        "docs": "https://docs.phalconphp.com/",
        "irc": "irc://irc.freenode.org/phalconphp",
        "rss": "https://blog.phalconphp.com/rss",
        "email": "support@phalconphp.com"
    },
    "require": {
        "php": ">=7.1",
        "ext-phalcon": ">=5.0.0"
    },
    "require-dev": {
        "codeception/verify": "*"
    },
    "autoload": {
        "psr-4": {
            "Phalcon\\Db\\": "Phalcon/Db/",
            "Phalcon\\Logger\\": "Phalcon/Logger/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phalcon\\Db\\": "Phalcon/Db/",
            "Phalcon\\Logger\\": "Phalcon/Logger/"
        }
    }
}
